        "editor.inactiveSelectionBackground": "#3f3980",
        "editorIndentGuide.background": "#131420",
        "editorIndentGuide.activeBackground": "#191825",
        "activityBar.background": "#131420",
        "activityBar.foreground": "#9391E6",
        "activityBarBadge.background": "#191825",
        "activityBarBadge.foreground": "#ffffff",
        "sideBar.background": "#131420",
        "sideBar.foreground": "#9391E6",
        "sideBarTitle.foreground": "#9391E6",
        "sideBarSectionHeader.background": "#131420",
        "sideBarSectionHeader.foreground": "#9391E6",
        "titleBar.activeBackground": "#131420",
        "titleBar.activeForeground": "#9391E6",
        "statusBar.background": "#131420",
        "statusBar.foreground": "#9391E6",
        "statusBar.noFolderBackground": "#131420",
        "tab.activeBackground": "#121019",
        "tab.inactiveBackground": "#131420",
        "tab.activeForeground": "#9391E6",
        "tab.inactiveForeground": "#7472C8",
        "list.activeSelectionBackground": "#191825",
        "list.activeSelectionForeground": "#ffffff",
        "list.hoverBackground": "#131420",
        "list.hoverForeground": "#9391E6",
        "list.inactiveSelectionBackground": "#131420",
        "input.background": "#3B3D62",
        "input.foreground": "#ffffff",
        "input.border": "#191825",
        "button.background": "#191825",
        "button.foreground": "#ffffff",
        "button.hoverBackground": "#1F1E2D",
        "panel.background": "#131420",
        "panel.border": "#191825",
        "panelTitle.activeForeground": "#313153",
        "panelTitle.inactiveForeground": "#7472C8",
        "badge.background": "#191825",
        "badge.foreground": "#ffffff",
        "scrollbarSlider.background": "#191825AA",
        "scrollbarSlider.hoverBackground": "#1F1E2DCC",
        "scrollbarSlider.activeBackground": "#191825AA",
        "editor.selectionHighlightBackground": "#191825",
        "selection.background": "#191825"
    },
    "tokenColors": [
        {
            "scope": "comment",
            "settings": {
                "foreground": "#A08088",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#9391E6"
            }
        },
        {
            "scope": ["keyword", "storage.type", "storage.modifier"],
            "settings": {
                "foreground": "#9391E6"
            }
        },
        {
            "scope": ["entity.name.function", "support.function"],
            "settings": {
                "foreground": "#9391E6"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#9391E6"
            }
        },
        {
            "scope": ["constant", "entity.name.type", "support.type"],
            "settings": {
                "foreground": "#9391E6"
            }
        },
        {
            "scope": ["punctuation", "meta.brace"],
            "settings": {
                "foreground": "#CCA2AD"
            }
        },
        {
            "scope": ["entity.name.tag", "support.class"],
            "settings": {
                "foreground": "#9391E6",
                "fontStyle": "bold"
            }
        }
    ]
}
